<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link width="" rel="shortcut icon" href="./images/logo-mercado-liebre.jpg" />

	<title> Mercado Liebre Argentina</title>
	<!-- Font Awesome Stylesheet -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css">
	<!-- Bootstrap Grid Stylesheet -->
	<link rel="stylesheet" href="./css/bootstrap-grid.min.css">
	<!-- Own Stylesheet -->
	<link rel="stylesheet" href="./css/app.css">
</head>

<body>
	<!-- Header -->
	<%- include('./partials/headerNoLogeadoi') %>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Registrate</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12"> 
                <form action="" method="GET">
                    <label for="email">Email: </label>
                    <input type="email" id="emial" name="email"  placeholder="Ingrese su emial">
                   
                    <label for="username">Usuario: </label>
                    <input type="text" id="username" name="username" placeholder="Ingrese su usuario">
                    
                    <label for="password">Ingrese su contraseña: </label>
                    <input type="text" id="password" name="password" placeholder="Ingrese su contraseña">
                    
                    <label for="FDN">Fecha de Nacimiento: </label>
                    <input type="date" id="FDN" name="FDN" placeholder="Ingrese si fecha de nacimiento">

                    <label for="NDD">Numero de Documento: </label>
                    <input type="number" id="NDD" name="NDD" placeholder="Ingrese su numero de documento">

                    <label for="FDP">Foto de perfil: </label>
                    <input type="text" id="FDP" name="FDP" >

                    <button type="submit">Registrarse</button>
                </form>
			</h2>


	<!-- footer -->
	<%- include('./partials/footer') %>
	<!-- /footer -->
</body>

</html>